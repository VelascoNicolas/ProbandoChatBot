{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**/*"]
            }
        }
    ],

    "installCommand": "npm i",

    "routes": [
        {
            "src": "/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "src/index.ts"
        }
    ]
}